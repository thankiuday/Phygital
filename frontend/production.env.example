# Production Environment Configuration for Frontend
# These variables should be set in Render dashboard for static sites

# Backend API URL (update this to your actual Render backend URL)
VITE_API_URL=https://phygital-backend.onrender.com/api

# Frontend URL (update this to your actual Render frontend URL)
VITE_FRONTEND_URL=https://phygital-frontend.onrender.com

# Environment
VITE_NODE_ENV=production

# Optional: Analytics and monitoring
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_ERROR_REPORTING=true

# Optional: Feature flags
VITE_ENABLE_PWA=false
VITE_ENABLE_OFFLINE_MODE=false

# Optional: Performance settings
VITE_ENABLE_SERVICE_WORKER=false
VITE_CACHE_DURATION=3600
