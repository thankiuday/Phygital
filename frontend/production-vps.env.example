# Frontend Production Environment Configuration for Hostinger VPS
# Copy this file to .env.production and fill in your actual values

# ============================================
# API Configuration
# ============================================
# Replace with your actual API domain
VITE_API_URL=https://api.yourdomain.com

# ============================================
# Application Configuration
# ============================================
VITE_APP_NAME=Phygital
VITE_APP_URL=https://yourdomain.com

# ============================================
# Feature Flags
# ============================================
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_AR=true

# ============================================
# File Upload Configuration
# ============================================
VITE_MAX_FILE_SIZE=104857600
VITE_ALLOWED_IMAGE_TYPES=image/jpeg,image/png,image/gif,image/webp
VITE_ALLOWED_MODEL_TYPES=model/gltf-binary,model/gltf+json

# ============================================
# AR Configuration
# ============================================
VITE_AR_MAX_SCALE=10
VITE_AR_MIN_SCALE=0.1
VITE_AR_DEFAULT_SCALE=1

# ============================================
# External Services (if any)
# ============================================
# Google Analytics (if needed)
# VITE_GA_TRACKING_ID=UA-XXXXXXXXX-X

# Sentry (for error tracking, if needed)
# VITE_SENTRY_DSN=your_sentry_dsn

# ============================================
# Build Configuration
# ============================================
VITE_BUILD_VERSION=1.0.0
VITE_BUILD_DATE=2025-10-28



