{
  "name": "phygital-backend",
  "version": "1.0.0",
  "description": "Backend for Phygital - Physical to Digital bridge platform",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build": "npm install",
    "test": "echo \"Error: no test specified\" && exit 1",
    "postinstall": "echo 'Backend dependencies installed successfully'",
    "cleanup:preview": "node scripts/production-cleanup.js --dry-run",
    "cleanup:db": "node scripts/production-cleanup.js --db-only --confirm",
    "cleanup:cloud": "node scripts/production-cleanup.js --cloud-only --confirm",
    "cleanup:all": "node scripts/production-cleanup.js --confirm",
    "cleanup:force": "node scripts/production-cleanup.js --confirm --skip-confirm"
  },
  "keywords": [
    "phygital",
    "qr",
    "video",
    "analytics"
  ],
  "author": "Phygital Team",
  "license": "MIT",
  "dependencies": {
    "aws-sdk": "^2.1467.0",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^2.7.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.10.0",
    "express-validator": "^7.0.1",
    "helmet": "^7.0.0",
    "jimp": "^0.22.10",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.5.0",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "node-fetch": "^2.7.0",
    "qrcode": "^1.5.3",
    "streamifier": "^0.1.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.10"
  }
}
